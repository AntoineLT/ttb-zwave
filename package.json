{
  "name": "ttb-zwave2",
  "version": "1.0.0",
  "description": "A Zwave Node-RED node made for TheThingBox and TimeSquAir.",
  "node-red": {
    "nodes": {
      "zwave": "zwave.js",
      "zwave-in": "zwave-in.js",
      "zwave-out": "zwave-out.js",
      "zwave-binary-switch": "zwave-binary-switch.js",
      "zwave-light-dimmer-switch": "zwave-light-dimmer-switch.js",
